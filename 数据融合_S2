    'F1B555', 
    'FCD163',
].join(',');
var vis_params = {min: 0, max: 11, palette: PALETTE};//类别有11类



//显示分类
Map.centerObject(stratifiedSamplepoints, 11);
Map.addLayer(imageclassified,vis_params,'imageclassified');



